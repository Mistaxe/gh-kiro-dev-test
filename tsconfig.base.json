{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2022", "DOM"],
      "strict": true,
      "useUnknownInCatchVariables": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "isolatedModules": true
    }
  }
  